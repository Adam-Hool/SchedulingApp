@{
    ViewBag.Title = "Requests";
}
<h2>@ViewBag.Title.</h2>

<div class="jumbotron">
    <h1>Event Calendar</h1>
    <div id="calendar2"></div>
</div>

@section scripts{

    <script type="text/javascript">


        $(document).ready(function () {
            $('#calendar2').fullCalendar({
                theme: true,

                //defaultView: 'agendaDay',
                editable: true,
                events: "/Requests/getevents/"
            });
        });


    </script>
}